{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "SimpleTaskHub"
    }
  },
  "logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}